<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ahsan_000.smileshine" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="Smile Shine" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BrushingActivity"
            android:label="Regular 2 Times Brushing " >
        </activity>
        <activity
            android:name=".FlossingActivity"
            android:label="Flossing Everyday" >
        </activity>
        <activity
            android:name=".WipingActivity"
            android:label="Wiping Mouth" >
        </activity>
        <activity
            android:name=".RinsingActivity"
            android:label="Mouth Rinsing" >
        </activity>
        <activity
            android:name=".HealthActivity"
            android:label="Diet and Oral Health" >
        </activity>
        <activity
            android:name=".DentalCheckUpActivity"
            android:label="Dental Check-Up" >
        </activity>
    </application>

</manifest>